{% extends "::layout.html.twig" %}

{% import '::actions.html.twig' as dedipanel_actions %}
{% import '::buttons.html.twig' as dedipanel_buttons %}

{% block javascripts %}
    <script type="text/javascript" src="{{ asset('bundles/dpgameserver/js/csgo-mod.js') }}"></script>
{% endblock %}

{% block title %}
    {{ dedipanel_buttons.back(path('dedipanel_steam_index'), 'steam.title.steam'|trans) }}
{% endblock %}
        
{% block content %}
    <h2>{{ 'steam.title.add'|trans }}</h2>

    <form action="{{ path('dedipanel_steam_create') }}" method="post" {{ form_enctype(form) }} id="form-create">
        {{ form_widget(form) }}
        {{ dedipanel_actions.create() }}
    </form>
{% endblock %}